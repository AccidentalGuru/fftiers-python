            var set = $(this).val();
            $("input[type='checkbox']").not('#mpb-roster, #mpb-taken, #mpb-available').attr('checked', false);
            $("."+set).prop("checked", true);
        }
    });

    $("#set-accuracy").change(function (e) {
        if (regfence(e)) {
            var year = $(this).val();
            $('.accuracy').hide();
            $("."+year).show();
        }
