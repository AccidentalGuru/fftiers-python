<script type="text/javascript">
$(document).ready(function() {

    var loggedin = (document.cookie.indexOf('fp_user') != -1) ? true : false;
    if (loggedin) {
        if ($('#mpb-roster').prop('checked') === false) {
            $('tr.mpb-roster').hide();
        }
        $('body').on('click', '#mpb-roster', function() {
            if ($('#mpb-roster').prop('checked') === false) {
                $('tr.mpb-roster, div.mpb-roster, li.mpb-roster').css({'display':'none'});
            } else {
                $('tr.mpb-roster').css({'display':'table-row'});
                $('div.mpb-roster').css({'display':'block'});
                $('li.mpb-roster').css({'display':'block'});
        }
        });

        if ($('#mpb-taken').prop('checked') === false) {
            $('tr.mpb-taken').hide();
        }
        $('body').on('click', '#mpb-taken', function() {
            if ($('#mpb-taken').prop('checked') === false) {
                $('tr.mpb-taken, div.mpb-taken, li.mpb-taken').css({'display':'none'});
            } else {
                $('tr.mpb-taken').css({'display':'table-row'});
                $('div.mpb-taken').css({'display':'block'});
                $('li.mpb-taken').css({'display':'block'});
            }
        });

        if ($('#mpb-available').prop('checked') === false) {
            $('tr.mpb-available').hide();
        }
        $('body').on('click', '#mpb-available', function() {
            if ($('#mpb-available').prop('checked') === false) {
                $('tr.mpb-available, div.mpb-available, li.mpb-available').css({'display':'none'});
            } else {
                $('tr.mpb-available').css({'display':'table-row'});
                $('div.mpb-available').css({'display':'block'});
                $('li.mpb-available').css({'display':'block'});
            }
        });

        var query = window.location.search.substring(1);
        var qs_vars = [];
        var qs_arr = [];

        if (query) {
            qs_vars = query.split('&');
        }

        var url = 'https://partners.fantasypros.com/api/c1/mpb-leagues.php?callback=callback&sport='+window.mpb_sport;

        var excluded_keys = ['team', 'league', 'myplaybook', 'key'];
        var item_k_v;
        for (var i = 0; i < qs_vars.length; i++) {
            item_k_v = qs_vars[i].split('=');
            if (excluded_keys.indexOf(item_k_v[0]) === -1) {
                qs_arr.push(item_k_v[0]+'='+item_k_v[1]);
            } else if (item_k_v[0] === 'myplaybook' || item_k_v[0] === 'key') {
                // append keys so changing the display league will work
                url += '&'+qs_vars[i];
            }
        }

        if (window.qs_extras && window.qs_extras.length > 0) {
            combined_qs_arr = qs_arr.concat(window.qs_extras);
        } else {
            combined_qs_arr = qs_arr;
        }
        var qs = combined_qs_arr.join('&');

        var mpb_html =  '<div class="pull-left">' +
                            '<div class="select-wrap" style="margin-right:5px;">' +
                                '<select class="select-links" name="mpb-league" id="mpb-set-league">' +

                                '</select>' +
                            '</div>' +
                            '<div class="select-wrap">' +
                                '<select class="select-links" name="mpb-team" id="mpb-set-team">' +

                                '</select>' +
                            '</div>' +
                        '</div>' +
                        '<div class="pull-right">' +
                            '<div class="mpb-options-form">' +
                                '<label class="mpb-roster-control"><input type="checkbox" id="mpb-roster" checked="checked"> <span>Roster</span></label>' +
                                '<label class="mpb-taken-control"><input type="checkbox" id="mpb-taken" checked="checked"> <span>Taken</span></label>' +
                                '<label class="mpb-available-control"><input type="checkbox" id="mpb-available" checked="checked"> <span>Available</span></label>' +
                             '</div>' +
                        '</div>';

        $('#mpb-controls').html(mpb_html);

        if (window.location.search.indexOf('roster=false') > -1) {
            $('#mpb-roster').prop('checked', false);
        }
        if (window.location.search.indexOf('taken=false') > -1) {
            $('#mpb-taken').prop('checked', false);
        }
        if (window.location.search.indexOf('available=false') > -1) {
            $('#mpb-available').prop('checked', false);
        }

        $.ajax({
            url: url,
            dataType: 'jsonp',
            type: 'GET',
            success: function(data) {
                var doc = document;
                var current_team;
                var current_league_name;
                var current_team_id;

                if (data['current_league']) {
                    $('.non-mpb').hide();
                    $('table.table.table-bordered').removeClass('table-striped').removeClass('table-hover');
                }

                var scoring = data.scoring;
                if(scoring) {
                    if(scoring != 'STD') {
                        $('#scoring-select').addClass('scoring-alert');
                    }
                }    

                var item, optVal;

                var leagues = data.leagues;
                if (typeof leagues === 'object' && Object.keys(leagues).length > 0) {
                     for (var lid in leagues) {
                        if (!leagues.hasOwnProperty(lid)) continue;
                        
                        var l = leagues[lid];
                        item = doc.createElement('option');
                        if (lid == data.current_league) {
                            current_team = l.team;
                            current_league_name = l.name;
                            item.setAttribute('selected', true);
                        } else {
                            optVal = '?myplaybook=display&key='+l.pf_key;
                            if (qs !== '') {
                                optVal +='&'+qs;
                            }
                            item.value = optVal;
                        }
                        
                        if (typeof l.nick !== 'undefined') {
                            item.innerHTML = l.nick;
                        } else {
                            item.innerHTML = l.name+' ('+l.host+')';
                        }
                        $('#mpb-set-league').append(item);
                    }
                    $('#mpb-set-league').append(' <option value="/'+window.mpb_sport+'/myplaybook/edit.php">+ Import a New Team</option>');
                    $('#mpb-set-league').show();
                }

                for (var i = 0; i < qs_vars.length; i++) {
                    item_k_v = qs_vars[i].split('=');
                    if (item_k_v[0] === 'team') {
                        current_team_id = item_k_v[1];
                        current_team = '';
                    }
                }

                var teams = data.teams;
                if (typeof teams === 'object' && Object.keys(teams).length > 0) {
                    for (var tid in teams) {
                        if (!teams.hasOwnProperty(tid)) continue;
                        
                        item = doc.createElement('option');
                        if (teams[tid] == current_team || tid == current_team_id) {
                            current_team_id = tid;
                            current_team = teams[tid];
                            item.setAttribute('selected', true);
                        } else {
                            optVal = '?team='+tid+'&league='+data.current_league;
                            if (qs !== '') {
                                optVal +='&'+qs;
                            }
                            item.value = optVal;
                        }
                        item.innerHTML = teams[tid];
                        $('#mpb-set-team').append(item);
                    }
                    $('#mpb-set-team').show();
                }

                $('tr[class*="mpb-player-"]').addClass('mpb-available');
                var owned_players = data.owned_players;
                if (typeof owned_players === 'object' && Object.keys(owned_players).length > 0) {
                    for (var pid in owned_players) {
                        if (!owned_players.hasOwnProperty(pid)) continue;
                        
                        $('.mpb-player-'+pid).removeClass('mpb-available');
                        if (owned_players[pid] == current_team_id) {
                            $('.mpb-player-'+pid).addClass('mpb-roster');
                        } else {
                            $('.mpb-player-'+pid).addClass('mpb-taken');
                        }
                    }
                }

                if (data.current_league) {
                    function mpb_hide(what) {
                        $('tr.mpb-'+what).css({'display':'none'});
                        $('div.mpb-'+what).css({'display':'none'});
                    }

                    $('.report-buttons').addClass('report-buttons-mpb');
                    $('#mpb-controls').show();
                    $('#rankings-table-wrap').css('margin-top', 0);
                } else {
                    $('#mpb-promo').show();
                    $('tr.mpb-available').removeClass('mpb-available');
                }
            },
            error: function(e) {
                $('#mpb-set-team, #mpb-set-league').hide();
            }
        });


        $('.mpb-link').click(function () {
            var link = $(this).attr('href');
            for (var i = 0; i < qs_vars.length; i++) {
                item_k_v = qs_vars[i].split('=');
                if (item_k_v[0] == 'team' | item_k_v[0] == 'league') {
                    if (link.indexOf('?') > 1) {
                        link += '&';
                    } else {
                        link += '?';
                    }
                    link += qs_vars[i];
                }
            }
            if ($('#mpb-roster').prop('checked') == false) {
                if (link.indexOf('?') > 1) {
                    link += '&roster=false';
                } else {
                    link += '?roster=false';
                }
            }
            if ($('#mpb-taken').prop('checked') == false) {
                if (link.indexOf('?') > 1) {
                    link += '&taken=false';
                } else {
                    link += '?taken=false';
                }
            }
            if ($('#mpb-available').prop('checked') == false) {
                if (query) {
                    link += '&';
                } else {
                    link += '?';
                }
                link += 'available=false';
            }
            $(this).attr('href', link);
        });

    }
});
</script>
